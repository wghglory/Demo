    <add name="SqlServerDB.Log" connectionString="Server=ghost-PC\SQL2008;Database=BeiDreamLog;Integrated Security=SSPI;" providerName="System.Data.SqlClient" />
    <add name="MySqlDB.Account" connectionString="Server=127.0.0.1;Database=BeiDreamAccount;Uid=root;Pwd=123456;" providerName="MySql.Data.MySqlClient" />
