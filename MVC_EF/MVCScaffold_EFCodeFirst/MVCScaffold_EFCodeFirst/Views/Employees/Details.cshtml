@model MVCScaffold_EFCodeFirst.Models.Employee

@{
    ViewBag.Title = "Details";
}

<h2>Details</h2>

<fieldset>
    <legend>Employee</legend>

    <div class="display-label">EmployeeName</div>
    <div class="display-field">@Model.EmployeeName</div>

    <div class="display-label">City</div>
    <div class="display-field">@Html.DisplayTextFor(_ => Model.City).ToString()</div>
</fieldset>
<p>
    @Html.ActionLink("Edit", "Edit", new { id=Model.EmployeeId }) |
    @Html.ActionLink("Back to List", "Index")
</p>


